{
  "name": "@gravitational/design-system",
  "version": "0.0.12",
  "main": "dist/index.js",
  "license": "Apache-2.0",
  "bin": {
    "design-system": "./cli/cli.js"
  },
  "scripts": {
    "prepare": "husky",
    "prebuild": "pnpm generate-theme",
    "build": "rm -rf dist && rollup -c rollup.config.mts && pnpm pack-release",
    "bot": "jiti bot/index.ts",
    "dev": "rollup -c rollup.config.mts -w",
    "eslint": "eslint . --ext .ts,.tsx",
    "prettier-check": "prettier --check \"**/*.{ts,tsx,mts,md,mdx}\"",
    "prettier-write": "prettier --log-level silent --write \"**/*.{ts,tsx,mts,md,mdx}\"",
    "lint": "pnpm eslint && pnpm prettier-check",
    "type-check": "tsc --build",
    "storybook": "storybook dev -p 6006",
    "prebuild-storybook": "pnpm generate-theme",
    "build-storybook": "storybook build",
    "test": "vitest --project=storybook && cd bot && pnpm test",
    "pack-release": "cd dist && pnpm pack --out package.tgz",
    "release": "changeset publish",
    "generate": "pnpm generate-icons && pnpm generate-theme && pnpm generate-props",
    "generate-icons": "jiti src/icons/generate.ts",
    "generate-props": "jiti src/storybook/props/generate.ts",
    "generate-theme": "chakra typegen src/themes/teleport/theme.ts --strict && jiti src/cli/generateComments.ts",
    "create-storybook-doc": "jiti src/storybook/create.ts",
    "taplo-check": "taplo fmt --check",
    "taplo-write": "taplo fmt"
  },
  "dependencies": {
    "@ark-ui/react": "^5.30.0",
    "@chakra-ui/cli": "^3.31.0",
    "@chakra-ui/react": "^3.31.0",
    "@hookform/resolvers": "^5.2.2",
    "@phosphor-icons/react": "^2.1.10",
    "react-hook-form": "^7.71.1",
    "next-themes": "^0.4.6"
  },
  "peerDependencies": {
    "@emotion/react": ">=11",
    "@fontsource/ubuntu": "^5.2.8",
    "react": ">=19",
    "react-dom": ">=19",
    "tslib": "^2.8.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^3.26.0",
    "@chakra-ui/react": "^3.26.0",
    "@changesets/cli": "^2.29.7",
    "@changesets/get-release-plan": "^4.0.13",
    "@changesets/types": "^6.1.0",
    "@emotion/react": "^11.14.0",
    "@eslint/js": "^9.39.1",
    "@fontsource/ubuntu": "^5.2.8",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@mdx-js/react": "^3.1.1",
    "@rollup/plugin-commonjs": "^28.0.9",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@storybook/addon-a11y": "^10.1.10",
    "@storybook/addon-docs": "^10.1.10",
    "@storybook/addon-themes": "^10.1.10",
    "@storybook/addon-vitest": "^10.1.10",
    "@storybook/react-vite": "^10.1.10",
    "@svitejs/changesets-changelog-github-compact": "^1.2.0",
    "@swc/core": "^1.15.2",
    "@taplo/cli": "^0.7.0",
    "@types/hast": "^3.0.4",
    "@types/node": "^24.10.1",
    "@types/prismjs": "^1.26.5",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "@vitest/browser": "^3.2.4",
    "@vitest/browser-playwright": "^4.0.12",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/eslint-plugin": "^1.4.3",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "6.0.0-rc.2",
    "eslint-plugin-testing-library": "^7.13.4",
    "globals": "^16.5.0",
    "hast": "^1.0.0",
    "husky": "^9.1.7",
    "jiti": "^2.6.1",
    "lint-staged": "^16.2.7",
    "markdown-to-jsx": "^7.7.17",
    "playwright": "^1.56.1",
    "prettier": "^3.6.2",
    "prismjs": "^1.30.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "rehype-github-alerts": "^4.1.1",
    "remark-gfm": "^4.0.1",
    "rollup": "^4.53.3",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-esbuild": "^6.2.1",
    "storybook": "^10.1.10",
    "tocbot": "^4.36.4",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0",
    "usehooks-ts": "^3.1.1",
    "vite": "^7.2.4",
    "vitest": "^4.0.12",
    "zod": "^4.3.6"
  },
  "packageManager": "pnpm@10.26.1+sha512.664074abc367d2c9324fdc18037097ce0a8f126034160f709928e9e9f95d98714347044e5c3164d65bd5da6c59c6be362b107546292a8eecb7999196e5ce58fa",
  "engines": {
    "node": "^22"
  },
  "storybook": {
    "title": "Teleport Design System",
    "url": "https://design.teleport.dev"
  },
  "publishConfig": {
    "directory": "dist"
  },
  "exports": {
    ".": {
      "dev": "./src/index.ts",
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./ubuntu.css": "./ubuntu.css"
  }
}

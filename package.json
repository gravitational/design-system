{
  "name": "@gravitational/design-system",
  "version": "0.0.1",
  "main": "dist/index.js",
  "bin": {
    "design-system": "./cli/cli.js"
  },
  "scripts": {
    "prepare": "husky",
    "build": "rm -rf dist && rollup -c rollup.config.mts",
    "dev": "rollup -c rollup.config.mts -w",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "prettier-check": "prettier --log-level silent --check \"**/*.{ts,tsx,mts,md,mdx}\"",
    "prettier-write": "prettier --log-level silent --write \"**/*.{ts,tsx,mts,md,mdx}\"",
    "lint": "pnpm eslint && pnpm prettier-check",
    "type-check": "tsc --build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest --project=storybook",
    "prerelease": "pnpm build",
    "release": "cd dist && pnpm publish --access public",
    "generate": "pnpm generate-icons",
    "generate-icons": "jiti src/icons/generate.ts",
    "generate-theme": "chakra typegen src/themes/teleport/theme.ts --strict && jiti src/cli/generateComments.ts"
  },
  "dependencies": {
    "@chakra-ui/cli": "^3.26.0",
    "@chakra-ui/react": "^3.26.0",
    "@phosphor-icons/react": "^2.1.10",
    "next-themes": "^0.4.6"
  },
  "peerDependencies": {
    "@emotion/react": ">=11",
    "react": ">=19",
    "react-dom": ">=19",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^3.26.0",
    "@chakra-ui/react": "^3.26.0",
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.29.7",
    "@changesets/get-release-plan": "^4.0.13",
    "@changesets/types": "^6.1.0",
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@commitlint/types": "^19.8.1",
    "@emotion/react": "^11.14.0",
    "@eslint/js": "^9.34.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@mdx-js/react": "^3.1.1",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@ryanclark/swc-plugin-jsx-marker": "1.0.0",
    "@storybook/addon-a11y": "^9.1.4",
    "@storybook/addon-docs": "^9.1.4",
    "@storybook/addon-themes": "^9.1.4",
    "@storybook/addon-vitest": "^9.1.4",
    "@storybook/react-vite": "^9.1.4",
    "@swc/core": "^1.13.5",
    "@types/prismjs": "^1.26.5",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react-swc": "^4.0.1",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/eslint-plugin": "^1.3.4",
    "eslint": "^9.34.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "6.0.0-rc.2",
    "eslint-plugin-testing-library": "^7.6.6",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jiti": "^2.5.1",
    "lint-staged": "^16.1.6",
    "markdown-to-jsx": "^7.7.13",
    "playwright": "^1.55.0",
    "prettier": "^3.6.2",
    "prismjs": "^1.30.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "rollup": "^4.49.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-esbuild": "^6.2.1",
    "storybook": "^9.1.4",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0",
    "usehooks-ts": "^3.1.1",
    "vite": "^7.1.4",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.14.0+sha512.ad27a79641b49c3e481a16a805baa71817a04bbe06a38d17e60e2eaee83f6a146c6a688125f5792e48dd5ba30e7da52a5cda4c3992b9ccf333f9ce223af84748",
  "engines": {
    "node": "^22"
  },
  "storybook": {
    "title": "Teleport Design System",
    "url": "https://design.teleport.dev"
  },
  "exports": {
    ".": {
      "dev": "./src/index.ts",
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  }
}

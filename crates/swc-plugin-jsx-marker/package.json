{
  "name": "@gravitational/swc-plugin-jsx-marker",
  "version": "0.0.1",
  "description": "SWC plugin for adding attributes to design system components",
  "main": "swc_plugin_jsx_marker.wasm",
  "scripts": {
    "build": "./build.sh",
    "build:debug": "cargo build -p swc_plugin_jsx_marker --target wasm32-wasip1 && cp ../../target/wasm32-wasip1/debug/swc_plugin_jsx_marker.wasm .",
    "test": "pnpm run build:debug && vitest run --testTimeout=0"
